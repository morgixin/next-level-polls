generator client {
  provider = "prisma-client-js"
}

datasource db {
  provider = "postgresql"
  url      = env("DATABASE_URL")
}

model Poll {
  id        String   @id @default(uuid()) // dificulta encontrar polls diferentes sรณ variando o id
  title     String
  createdAt DateTime @default(now())
  updatedAt DateTime @default(now())
}
